{
  "name": "redux-crud-manager",
  "version": "0.4.1",
  "description": "Redux CRUD utilities for keep store sync with your server",
  "main": "dist/ReduxCrudManager.js",
  "scripts": {
    "build": "webpack --env build",
    "build-all": "webpack --env dev && webpack --env build",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --compilers js:babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --compilers js:babel-core/register --colors -w ./test/*.spec.js",
    "prepublishOnly": "npm run build-all"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "chai": "4.1.2",
    "eslint": "4.13.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-import-resolver-webpack": "0.8.3",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.8.0",
    "immutability-helper": "2.6.2",
    "mocha": "4.0.1",
    "pubsub-js": "1.5.8",
    "uniqid": "4.1.1",
    "webpack": "3.10.0",
    "yargs": "10.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guillaumejasmin/redux-crud-manager.git"
  },
  "keywords": [
    "redux",
    "redux-crud-manager"
  ],
  "author": "Guillaume Jasmin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guillaumejasmin/redux-crud-manager/issues"
  },
  "homepage": "https://github.com/guillaumejasmin/redux-crud-manager",
  "config": {
    "unsafe-perm": true
  }
}
